    "string-similarity": "4.0.4",
    "utf-8-validate": "6.0.3",
    "uuid": "9.0.1"
  },
  "resolutions": {
    "strip-ansi": "6.0.1",
    "string-width": "4.2.2",
    "wrap-ansi": "7.0.0"
